# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Setup endpoints

GET     /v1/event/:id                             no.uio.musit.microservice.event.resource.EventResource.getEvent(id:Long)
POST    /v1/event                                 no.uio.musit.microservice.event.resource.EventResource.postEvent
GET     /v1/events       no.uio.musit.microservice.event.resource.EventResource.getEvents(search: Option[no.uio.musit.microservices.common.domain.MusitSearch])

POST    /v1/node/:id/control    no.uio.musit.microservice.event.resource.EventResource.postControl(id: Int)




