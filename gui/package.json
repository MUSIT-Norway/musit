{
  "name": "MUSIT-Frontend",
  "version": "1.0.0",
  "description": "ReactJS app to communicate with the MUSIT-Backend.",
  "main": "server/main.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "better-npm-run start-prod",
    "start-prod-api": "better-npm-run start-prod-api",
    "build": "better-npm-run build",
    "postinstall": "npm run build",
    "lint": "eslint --fix --config=./.eslintrc . --ext=jsx --ext=js --ignore-path .eslintignore app server",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run start-dev-api",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "karma": "karma start",
    "test": "npm run test-server && npm run test-client",
    "test-client": "./node_modules/mocha/bin/mocha $(find app -name '*-test.js') --compilers js:babel-core/register",
    "test-server": "./node_modules/mocha/bin/mocha $(find app-api -name '*-test.js') --compilers js:babel-core/register",
    "test-server-watch": "./node_modules/mocha/bin/mocha $(find app-api -name '*-test.js') --compilers js:babel-core/register --watch"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./server/main.js",
      "env": {
        "NODE_PATH": "./app",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-prod-api": {
      "command": "node ./server/main-api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./server/main.js",
      "env": {
        "NODE_PATH": "./app",
        "NODE_ENV": "development",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "node ./server/main-api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./app",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/production.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MUSIT-Norway/musit"
  },
  "homepage": "http://musit.uio.no",
  "keywords": [
    "MUSIT",
    "Frontend",
    "react",
    "Museum",
    "natural",
    "cultural",
    "history",
    "redux",
    "Museum",
    "webpack",
    "node",
    "gateway",
    "express",
    "babel"
  ],
  "author": "MUSIT",
  "license": "SEE LICENSE IN ../LICENSE.md",
  "dependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "compression": "^1.6.1",
