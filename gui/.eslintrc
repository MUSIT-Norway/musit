{ "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "plugins": [
    "react",
    "import"
  ],
  "rules": {
    "react/no-multi-comp": 1,
    "import/default": 1,
    "import/no-duplicates": 1,
    "import/named": 1,
    "import/namespace": 1,
    "import/no-unresolved": 1,
    "import/no-named-as-default": 1,
    "comma-dangle": 1,
    "indent": 1,
    "no-console": 1,
    "no-alert": 2,
    "react/jsx-indent": 1,
    "no-mixed-spaces-and-tabs": 1,
    "quote-props": 1,
    "arrow-body-style": 1,
    "no-param-reassign": 1,
    "no-var": 1,
    "global-require": 1,
    "no-unused-vars": 1,
    "max-len": 1,
    "object-shorthand": 1,
    "prefer-template": 1,
    "no-return-assign": 1,
    "no-confusing-arrow": 1,
    "react/self-closing-comp": 1,
    "react/prefer-stateless-function": 1,
    "no-underscore-dangle": 1,
    "prefer-arrow-callback": 1,
    "space-before-function-paren": 1,
    "vars-on-top": 1,
    "no-shadow": 1,
    "prefer-const": 1,
    "eqeqeq": 1
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "settings": {
    "import/resolve": {
      "moduleDirectory": ["node_modules", "src"]
    }
  },
  "globals": {
    "__DEVELOPMENT__": true,
    "__CLIENT__": true,
    "__SERVER__": true,
    "__DISABLE_SSR__": true,
    "__DEVTOOLS__": true,
    "socket": true,
    "webpackIsomorphicTools": true
  }
}
