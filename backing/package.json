{
  "name": "MUSIT-Backing",
  "version": "1.0.0",
  "description": "The MUSIT Backing services.",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "better-npm-run start-prod",
    "start-prod-api": "better-npm-run start-prod-api",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run start-dev-api",
    "dev": "concurrently --kill-others \"npm run start-dev\" \"npm run start-dev-api\""
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./scripts/api.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": 9090,
        "APIPORT": 3030
      }
    },
    "start-prod-api": {
      "command": "node ./scripts/gateway.js",
      "env": {
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./scripts/api.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 9090,
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "node ./scripts/gateway.js",
      "env": {
        "NODE_ENV": "development",
        "APIPORT": 3030
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MUSIT-Norway/musit"
  },
  "homepage": "http://musit.uio.no",
  "keywords": [
    "MUSIT",
    "Frontend",
    "react",
    "Museum",
    "natural",
    "cultural",
    "history",
    "redux",
    "Museum",
    "webpack",
    "node",
    "gateway",
    "express",
    "babel"
  ],
  "author": "MUSIT",
  "license": "SEE LICENSE IN ../LICENSE.md",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-plugin-typecheck": "^3.8.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "better-npm-run": "0.0.8",
    "compression": "^1.6.1",
    "concurrently": "^2.0.0",
    "ejs": "^2.4.2",
    "express": "^4.14.0",
    "http-proxy": "^1.13.2",
    "log": "^1.4.0",
    "node-cache": "^3.2.1",
    "passport": "^0.3.2",
    "passport-dataporten": "^1.1.3",
    "passport-http-bearer": "^1.0.1",
    "request": "^2.72.0",
    "stringify-object": "^2.4.0",
    "superagent": "^2.0.0"
  }
}
