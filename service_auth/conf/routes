# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET        /web/museum/:museumId/group/list               controllers.web.GroupController.groupList(museumId: Int)
GET        /web/museum/:museumId/group/add                controllers.web.GroupController.groupAddGet(museumId: Int)
POST       /web/museum/:museumId/group/add                controllers.web.GroupController.groupAddPost(museumId: Int)
GET        /web/museum/:museumId/group/:groupId/add       controllers.web.GroupController.groupAddActorGet(museumId: Int, groupId: String)
POST       /web/museum/:museumId/group/:groupId/add       controllers.web.GroupController.groupAddActorPost(museumId: Int, groupId: String)
GET        /web/museum/:museumId/group/:groupId           controllers.web.GroupController.groupActorsList(museumId: Int, groupId: String)